<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="AppXF.Themes.DarkTheme">
    <Color x:Key="PageBackgroundColor">#33334d</Color>
    <Color x:Key="NavigationBarColor">Teal</Color>
    <Color x:Key="PrimaryColor">Teal</Color>
    <Color x:Key="SecondaryColor">White</Color>
    <Color x:Key="PrimaryTextColor">White</Color>
    <Color x:Key="SecondaryTextColor">White</Color>
    <Color x:Key="TransparentColor">Transparent</Color>
    <Color x:Key="SwitchOnColor">MediumSpringGreen</Color>
    <Color x:Key="SwitchOffColor">Red</Color>
    <Color x:Key="ButtonBackgroundColor">#8080ff</Color>
    <Color x:Key="ButtonTextColor">White</Color>
</ResourceDictionary>